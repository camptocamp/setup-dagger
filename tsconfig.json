{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": [
		"@tsconfig/strictest/tsconfig.json"
	],
	"include": [
		"./src"
	],
	"compilerOptions": {
		"module": "ES2022",
		"target": "ES2022",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"noImplicitAny": true,
		"noFallthroughCasesInSwitch": true
	}
}
